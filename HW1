<!DOCTYPE html> <html> <body> <h2>Program Synthesis HW1</h2> <p id="demo"></p> <script> let input = prompt("Enter as many numbers as you please, separated by spaces:"); let numbers = input.split(" ").map(Number); let table = "<table><tbody>"; for (let i = 0; i < numbers.length; i += 2) { table += "<tr><td>" + numbers[i] + "</td><td>" + (numbers[i + 1] || "") + "</td></tr>"; } table += "</tbody></table>"; function check(x, y, z) { for (let i = 0; i < z.length; i+=2) { let sum = (z[i]*x) + (y*1); if (sum !== z[i+1]) { return false; } } return true; } function check2(x, y, z) { for (let i = 0; i < z.length; i+=2) { let sum = (z[i]/x)-(y*1); if (sum !== z[i+1]) { return false; } } return true; } let text = "The following are functions that satisfy the given input and output pairs depicted in the table below. 'a' is the given input:" + "<br>"; let a = numbers[0]; let b = numbers[1]; let value = 0; let value2 = 0; for(let j=0; j<11; j++){ for(let k=0; k<11; k++){ value = (a*j)+(1*k); if (j>0){ value2 = (a/j)-(1*k); } if (value == b && check(j,k,numbers)){ text += "(a*" +j+")" + "+" + "(1*"+ k + ")"+"<br>"; } if (value2 == b && check2(j,k,numbers)) text += "(a/"+j+")" + "-" + "(1*"+ k + ")"+"<br>"; } }     document.getElementById("demo").innerHTML = text; document.write(table); </script> </body> </html>
